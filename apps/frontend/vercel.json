{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.tsuwari.tk/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://api.tsuwari.tk/:path*/"
    },
    {
      "source": "/socket.io/",
      "destination": "https://api.tsuwari.tk/socket.io/"
    },
    {
      "source": "/socket.io/:path*/",
      "destination": "https://api.tsuwari.tk/socket.io/:path*/"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://api.tsuwari.tk/socket.io/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}